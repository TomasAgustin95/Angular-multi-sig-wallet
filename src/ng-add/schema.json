{
  "$schema": "http://json-schema.org/schema",
  "$id": "options",
  "title": "Angualr on Chain",
  "type": "object",
  "properties": {
    "configuration": {
      "type": "string",
      "description": "description",
      "uniqueItems": true,
      "default":"minimalContract",
      "enum": ["minimalContract","helloWorldContract","debugContract", "nftContract"]
  },
  "skipInstall": {
    "description": "skipInstall",
    "type": "boolean",
    "default": false
  },
  "test": {
    "description": "Am i in test?",
    "type": "boolean",
    "default": false
  },
  "alreadyInstalled": {
    "description": "alreadyInstalled",
    "type": "boolean",
    "default": false
  }
}
}